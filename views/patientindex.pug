      .col-md
        .ftco-footer-widget.mb-4
          h2.ftco-heading-2 Site Links
          ul.list-unstyled
            li
              a.py-2.d-block(href='#') Home
            li
                a.py-2.d-block(href='#') Laboratory Services
            li
                a.py-2.d-block(href='#') General Services
            li
                a.py-2.d-block(href='#') 24/7 Help and Support

      .col-md
        .ftco-footer-widget.mb-4
          h2.ftco-heading-2 Have a Question?
          .block-23.mb-3
            ul
              li
                span.icon.icon-map-marker
                span.text Philadelphia, PA 19104
              li
                a(href='#')
                  span.icon.icon-phone
                  span.text +1 800 420 4357
              li
                a(href='#')
                  span.icon.icon-envelope
                  span.text info@waitER.com
    .row
      .col-md-12.text-center
