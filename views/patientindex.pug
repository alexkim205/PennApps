// loader
#ftco-loader.show.fullscreen
  svg.circular(width='48px' height='48px')
    circle.path-bg(cx='24' cy='24' r='22' fill='none' stroke-width='4' stroke='#eeeeee')
    circle.path(cx='24' cy='24' r='22' fill='none' stroke-width='4' stroke-miterlimit='10' stroke='#F96D00')
// Modal
#modalAppointment.modal.fade(tabindex='-1' role='dialog' aria-labelledby='modalAppointmentLabel' aria-hidden='true')
  .modal-dialog(role='document')
    .modal-content
      .modal-header
        h5#modalAppointmentLabel.modal-title Appointment
        button.close(type='button' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body
        form(action='#')
          .form-group
            label.text-black(for='appointment_name') Full Name
            input#appointment_name.form-control(type='text')
          .form-group
            label.text-black(for='appointment_email') Email
            input#appointment_email.form-control(type='text')
          .row
            .col-md-6
              .form-group
                label.text-black(for='appointment_date') Date
                input#appointment_date.form-control(type='text')
            .col-md-6
              .form-group
                label.text-black(for='appointment_time') Time
                input#appointment_time.form-control(type='text')
          .form-group
            label.text-black(for='appointment_message') Message
            textarea#appointment_message.form-control(name='' cols='30' rows='10')
          .form-group
            input.btn.btn-primary(type='submit' value='Send Message')
